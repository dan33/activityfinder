<!DOCTYPE html>
<html>
  <head>
    <title>ActivityFinder</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>

      <div id="user_nav">
        <%= render :partial => 'layouts/intellinav' %>
      </div>
      <% flash.each do |name, msg| %>
        <%= content_tag :div, msg, id: "flash_#{name}" %>
      <% end %>

    <%= render :partial => 'activities/all', :locals => {:activities => @activities } %>

    <%= yield %>
  </body>
</html>
