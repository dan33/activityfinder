%div#map_float
  %div#map

%div#container
  %div#profilepic
    -if current_user.present?
      = image_tag(current_user.image, :id => 'ppic')
  %div#buttons
    %div#button1
      -if current_user.present?
        = link_to("Profile", "#")
    %div#button2
%div#panel
  - if current_user.present?
    = render 'users/show'


%div#search
  = render 'activities/search'