%div#map_float
  %div#map

-if current_user.present?
  %div#container
    %div#profilepic
      -if current_user.present?
        = image_tag(current_user.image, :id => 'ppic')
    %div#buttons
      %div#button1
        -if current_user.present?
          %p CREATE ACTIVITY
      %div#button2
        -if current_user.present?
          %p PROFILE
  %div#panel.hidden
    - if current_user.present?
      = render 'users/show'


%div#search
  = render 'activities/search'